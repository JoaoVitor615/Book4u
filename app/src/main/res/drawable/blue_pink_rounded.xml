<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="#919191">

    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/Rosa"
                android:endColor="@color/Azul"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>

    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/ColorWhite" />
            <corners android:radius="6dp"/>
        </shape>
    </item>

</ripple>